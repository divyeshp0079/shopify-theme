{"sections":{"main":{"type":"main-product","blocks":{"title_XwTchD":{"type":"title","settings":{}},"custom_liquid_pymx63":{"type":"custom_liquid","settings":{"custom_liquid":"<script>\ndocument.addEventListener('DOMContentLoaded', () => {\n\n  const siblings = document.querySelectorAll('.js-color-sibling');\n  const variantRadios = document.querySelectorAll(\n    'variant-radios input[type=\"radio\"]'\n  );\n\n  if (!siblings.length || !variantRadios.length) return;\n\n  siblings.forEach(sibling => {\n    sibling.addEventListener('click', e => {\n\n      \/* ðŸ”¥ PAGE RELOAD STOP *\/\n      e.preventDefault();\n      e.stopPropagation();\n\n      const color = sibling.dataset.color;\n      if (!color) return;\n\n      \/* UI active state *\/\n      siblings.forEach(s => s.classList.remove('active'));\n      sibling.classList.add('active');\n\n      \/* ðŸ”¥ REAL VARIANT CHANGE *\/\n      variantRadios.forEach(radio => {\n        if (radio.value.toLowerCase() === color) {\n          radio.checked = true;\n          radio.dispatchEvent(new Event('change', { bubbles: true }));\n          radio.dispatchEvent(new Event('input', { bubbles: true }));\n        }\n      });\n\n      \/* ðŸ”¥ URL CHANGE WITHOUT RELOAD *\/\n      const targetUrl = sibling.dataset.url;\n      if (targetUrl) {\n        window.history.pushState({}, '', targetUrl);\n      }\n\n    });\n  });\n\n});\n<\/script>"}},"new_aaps_star_rating_iwymCp":{"type":"shopify:\/\/apps\/new-aaps\/blocks\/star_rating\/019bb71f-104d-78a1-9adf-b62e18c95cd5","settings":{"colour":"#000000"}},"variant_picker":{"type":"variant_picker","disabled":true,"settings":{"picker_type":"button"}},"custom_liquid_czM8jq":{"type":"custom_liquid","disabled":true,"settings":{"custom_liquid":"<style>\n\/* thumbnail swatches style *\/\n.variant-image-swatches { \n  list-style: none; \n  padding: 0; \n  margin: 12px 0; \n  display: flex; \n  gap: 10px; \n  align-items: center; \n}\n.variant-image-swatches .variant-image-swatch { \n  cursor: pointer; \n  border: 2px solid transparent; \n  padding: 4px; \n  border-radius: 8px; \n  background: #fff; \n  box-shadow: 0 1px 2px rgba(0,0,0,0.05); \n  transition: all 0.2s ease;\n}\n.variant-image-swatches .variant-image-swatch img { \n  display: block; \n  width: 56px; \n  height: 56px; \n  object-fit: cover; \n  border-radius: 6px; \n}\n.variant-image-swatches .variant-image-swatch.selected { \n  border-color: #111; \n  box-shadow: 0 3px 8px rgba(0,0,0,0.12); \n  transform: translateY(-2px); \n}\n<\/style>\n\n<ul class=\"variant-image-swatches\" aria-label=\"Variant images\">\n  {% for variant in product.variants %}\n    {% assign v_image = variant.image | default: product.featured_image %}\n    <li \n      class=\"variant-image-swatch\" \n      data-variant-id=\"{{ variant.id }}\" \n      data-image-src=\"{{ v_image | img_url: '1024x' }}\" \n      data-price=\"{{ variant.price | money }}\" \n      data-title=\"{{ variant.title }}\" \n      data-available=\"{{ variant.available }}\" \n      title=\"{{ variant.title }}\">\n      <img src=\"{{ v_image | img_url: '200x' }}\" alt=\"{{ variant.title }}\">\n    <\/li>\n  {% endfor %}\n<\/ul>\n\n<script>\n(function(){\n  var swatches = document.querySelectorAll('.variant-image-swatch');\n  if(!swatches.length) return;\n\n  function clearSelected(){\n    swatches.forEach(s => s.classList.remove('selected'));\n  }\n\n  var variantInput = document.querySelector('form[action*=\"\/cart\/add\"] input[name=\"id\"], form.product-form input[name=\"id\"]');\n  var variantSelect = document.querySelector('form[action*=\"\/cart\/add\"] select[name=\"id\"], form.product-form select[name=\"id\"]');\n  var mainImage = document.querySelector('.product__media img, .product-single__photo img, .product-featured img, .product__gallery img, .product-main-image img');\n  var priceEl = document.querySelector('[data-price], .price-item--regular, .product__price, .price, [id^=\"ProductPrice\"]');\n  var titleEl = document.querySelector('.product__title, .product-single__title, h1.product-title');\n\n  swatches.forEach(function(swatch){\n    swatch.addEventListener('click', function(){\n      var variantId = this.dataset.variantId;\n      var imageSrc = this.dataset.imageSrc;\n      var price = this.dataset.price;\n      var title = this.dataset.title;\n      var available = this.dataset.available === \"true\";\n\n      clearSelected();\n      this.classList.add('selected');\n\n      if(imageSrc && mainImage){\n        if(mainImage.tagName.toLowerCase() === 'img'){\n          mainImage.src = imageSrc;\n          mainImage.removeAttribute('srcset');\n        } else {\n          mainImage.style.backgroundImage = `url(${imageSrc})`;\n        }\n      }\n\n      if(variantInput){\n        variantInput.value = variantId;\n        variantInput.dispatchEvent(new Event('change', { bubbles: true }));\n        variantInput.dispatchEvent(new Event('input', { bubbles: true }));\n      }\n      if(variantSelect){\n        var opt = variantSelect.querySelector(`option[value=\"${variantId}\"]`);\n        if(opt){\n          variantSelect.value = variantId;\n          variantSelect.dispatchEvent(new Event('change', { bubbles: true }));\n        }\n      }\n\n      \/\/ Update product title and price dynamically\n      if(titleEl) titleEl.textContent = title;\n      if(priceEl) priceEl.textContent = price;\n\n      \/\/ Update \"Sold Out\" button state dynamically\n      var addToCartBtn = document.querySelector('form[action*=\"\/cart\/add\"] [type=\"submit\"], .product-form__submit, #AddToCart');\n      if(addToCartBtn){\n        addToCartBtn.disabled = !available;\n        addToCartBtn.textContent = available ? \"Add to cart\" : \"Sold out\";\n      }\n\n      window.dispatchEvent(new CustomEvent('variantImageSwatch:changed', { detail: { variantId } }));\n    });\n  });\n\n  \/\/ Mark default variant selected on page load\n  (function markInitial(){\n    var currentId = variantInput?.value || variantSelect?.value;\n    if(currentId){\n      var selected = document.querySelector(`.variant-image-swatch[data-variant-id=\"${currentId}\"]`);\n      if(selected){\n        clearSelected();\n        selected.classList.add('selected');\n      }\n    }\n  })();\n})();\n<\/script>"}},"custom_liquid_jke3Pi":{"type":"custom_liquid","disabled":true,"settings":{"custom_liquid":"<style>\n\/* thumbnail swatches style *\/\n.variant-image-swatches { \n  list-style: none; \n  padding: 0; \n  margin: 12px 0; \n  display: flex; \n  gap: 10px; \n  align-items: center; \n}\n.variant-image-swatches .variant-image-swatch { \n  cursor: pointer; \n  border: 2px solid transparent; \n  padding: 4px; \n  border-radius: 8px; \n  background: #fff; \n  box-shadow: 0 1px 2px rgba(0,0,0,0.05); \n  transition: all 0.2s ease;\n}\n.variant-image-swatches .variant-image-swatch img { \n  display: block; \n  width: 56px; \n  height: 56px; \n  object-fit: cover; \n  border-radius: 6px; \n}\n.variant-image-swatches .variant-image-swatch.selected { \n  border-color: #111; \n  box-shadow: 0 3px 8px rgba(0,0,0,0.12); \n  transform: translateY(-2px); \n}\n<\/style>\n\n<ul class=\"variant-image-swatches\" aria-label=\"Variant images\">\n  {% for variant in product.variants %}\n    {% assign v_image = variant.image | default: product.featured_image %}\n    <li \n      class=\"variant-image-swatch\" \n      data-variant-id=\"{{ variant.id }}\" \n      data-image-src=\"{{ v_image | img_url: '1024x' }}\" \n      data-price=\"{{ variant.price | money }}\" \n      data-title=\"{{ variant.title }}\" \n      data-available=\"{{ variant.available }}\" \n      title=\"{{ variant.title }}\">\n      <img src=\"{{ v_image | img_url: '200x' }}\" alt=\"{{ variant.title }}\">\n    <\/li>\n  {% endfor %}\n<\/ul>\n\n<script>\n(function(){\n  var swatches = document.querySelectorAll('.variant-image-swatch');\n  if(!swatches.length) return;\n\n  function clearSelected(){\n    swatches.forEach(s => s.classList.remove('selected'));\n  }\n\n  var 